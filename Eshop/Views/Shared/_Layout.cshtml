                        </div>
                    </div>
                </div>
            </div>
            <hr />
            <div class="footermainwrapper2">
                <p>Copyright © Vesoz All Rights Reserved.</p>
                <div class="footerwrapper5">
                    <ul class="dotslist">
                        <li><a href="https://www.facebook.com/"><div class="circle" id="fb"></div></a></li>
                        <li><a href="https://twitter.com/"><div class="circle" id="tw"></div></a></li>
                        <li><a href="https://aboutme.google.com/u/0/?referer=gplus"><div class="circle" id="gp"></div></a></li>
                        <li><a href="https://www.youtube.com/"><div class="circle" id="yt"></div></a></li>
                    </ul>
                </div>
            </div>
        </footer>
    </div>


    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>