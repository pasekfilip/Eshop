                    <div class="footersubwrappers">
                        <p class="headings">Email:</p>
                        <a class="email" href="mailto:example@example.com">example@example.com</a>
                    </div>
                </div>
                <div class="footerwrapper2">
                    <p class="headings2">About Us</p>
