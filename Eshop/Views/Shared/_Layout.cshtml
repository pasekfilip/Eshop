@{ 
    int count = 0;
    if(Session["listOfProducts"] != null)
    {
        var session = ((List<Eshop.Models.Tables.ProductLabelImages>)Session["listOfProducts"]);
        foreach (var item in session)
        {
            count += item.Quantity;
        }
    }
}
<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    @Scripts.Render("~/Scripts/jquery-3.4.1.min.js")
    @Scripts.Render("~/Scripts/jquery.unobtrusive-ajax.min.js")
    @Scripts.Render("~/Scripts/script.js")
</head>
<body>
    <div class="container">
        <div class="navbar1">
            <div class="gridmaincontainer1">
                <div class="gridcontainer1">
                    <ul class="sociallist">
                        <li class="socialli">
                            <p class="follow">Follow Us:</p>
                        </li>
                        <li class="socialli">
                            <a href="https://www.facebook.com/"><img class="socialmedia" src="~/Icons/facebook_white.png" alt="Facebook" /></a>
                        </li>
                        <li class="socialli">
                            <a href="https://twitter.com/"><img class="socialmedia" src="~/Icons/twitter_white.png" alt="Twitter" /></a>
                        </li>
                        <li class="socialli">
                            <a href="https://aboutme.google.com/u/0/?referer=gplus"><img class="socialmedia" src="~/Icons/googleplus_white.png" alt="Google Plus" /></a>
                        </li>
                        <li class="socialli">
                            <a href="https://www.instagram.com/"><img class="socialmedia" src="~/Icons/instagram_white.png" alt="Instagram" /></a>
                        </li>
                        <li class="socialli">
                            <a href="https://www.youtube.com/"><img class="socialmedia" src="~/Icons/youtube_white.png" alt="Youtube" /></a>
                        </li>
                    </ul>
                </div>
                <div class="accountcurrency">
                    <a href="/Login">My Account</a>
                    <span>|</span>
                    <a href="#">USD</a>
                </div>
            </div>
            <hr />
            <div class="gridmaincontainer2">
                <div class="gridcontainer3">
                    <a href="/Home" class="debralogo"><img src="~/Icons/debra-logo-w_300x.png" alt="Debra logo" /></a>
                    <div class="searchwrapper">
                        <input type="text" class="searchbar" name="name" placeholder="Search our store" value="" />
                        <input type="submit" class="searchbutton" name="search" value="" />
                    </div>
                    <a href="#" class="cart">
                        <div class="carta"></div>
                        <div class="cartb">
                            <div class="dot">
                                <p>@count</p>
                            </div>
                        </div>
                    </a>
                </div>
            </div>
        </div>
        <div class="navbar2">
            <a class="navbara">@Html.ActionLink("Home", "Index", "Home")</a>
            <a class="navbara">@Html.ActionLink("Shop", "About", "Home")</a>
            <a class="navbara">@Html.ActionLink("Products", "Contact", "Home")</a>
            <a class="navbara">@Html.ActionLink("Blog", "Index", "Home")</a>
            <a class="navbara">@Html.ActionLink("About", "About", "Home")</a>
            <a class="navbara">@Html.ActionLink("Contact", "Contact", "Home")</a>
        </div>
    </div>
    <div class="bodymain">
        @RenderBody()
        <footer>
            <div class="footermainwrapper1">
                <div class="footerwrapper1">
                    <img class="logo" src="~/Icons/debra-logo-b_300x.png" alt="Debra" />
                    <div class="footersubwrappers">
                        <p class="headings">Address:</p>
                        <a class="address" href="geo:4710-4890 Breckinridge St, UK Burlington, VT 05401">4710-4890 Breckinridge St, UK Burlington, VT 05401</a>
                    </div>
                    <div class="footersubwrappers">
                        <p class="headings">Need Help?</p>
                        <a class="tel" href="tel:18003456789">Call: 1-800-345-6789</a>
